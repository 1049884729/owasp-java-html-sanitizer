<!-- -*- mode: xml-mode; indent-tabs-mode: nil; -*- -->
<!-- This maintains a list of the published versions.
  - Each version corresponds to a sibling directory.
  - See the README in the root maven directory for an explanation of the
  - strange syntactic features in this file.
  -->
<metadata>
  <groupId>owasp-java-html-sanitizer</groupId>
  <artifactId>owasp-java-html-sanitizer</artifactId>
  <!-- Version numbers are based on revision control labels -->
  <versioning>
    <!-- Disabled until we cut first release. -->
    <!--##REPLACE##<release>+++version+++</release>##END##-->
    <versions>
      <!--##DUPE##<version>+++version+++</version>##END##-->
      <!-- The line above is a template for generating a new version by the
        - script that cuts a maven release. -->
    </versions>
    <!--##REPLACE##<lastUpdated>+++timestamp+++</lastUpdated>##END##-->
    <!-- The line above is replaced with the time the last release was cut in
      - YYYYMMDDhhmmss format -->
  </versioning>
</metadata>
